<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--   your app branding color for the app bar -->
        <item name="colorPrimary">#009688</item>
        <!--   darker variant for the status bar and contextual app bars -->
        <item name="colorPrimaryDark">#004D40</item>
        <!--   theme UI controls like checkboxes and text fields -->
        <item name="colorAccent">#F06292</item>
    </style>

    <style name="LightTeal" parent="AppTheme" >
        <item name="colorPrimary">#B2DFDB</item>
        <item name="colorPrimaryDark">#4DB6AC</item>
        <item name="colorAccent">#F48FB1</item>
    </style>

</resources>